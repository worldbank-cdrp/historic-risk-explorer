{
  "name": "historic-risk-explorer",
  "version": "0.0.1",
  "repository": "https://github.com/worldbank-cdrp/historic-risk-explorer.git",
  "author": {
    "name": "Development Seed",
    "url": "https://developmentseed.org"
  },
  "scripts": {
    "postinstall": "[ -f app/assets/scripts/config/local.js ] || echo \"'use strict';\n\nexport default {\n  environment: 'development'\n};\" > app/assets/scripts/config/local.js",
    "serve": "DS_ENV=development gulp collecticons && gulp serve",
    "build": "DS_ENV=production NODE_ENV=production gulp",
    "stage": "DS_ENV=staging NODE_ENV=staging gulp",
    "lint": "eslint app/**/*.js",
    "test": "echo \"No tests\" && exit 0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ],
          "plugins": [
            "transform-class-properties"
          ]
        }
      ],
      "require-globify",
      "envify"
    ]
  },
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "babelify": "^7.3.0",
    "browser-sync": "^2.18.8",
    "browserify": "^14.1.0",
    "collecticons-processor": "^3.1.0",
    "del": "^2.2.2",
    "envify": "^4.0.0",
    "eslint": "^3.18.0",
    "eslint-config-standard": "^7.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^2.1.1",
    "gulp": "^3.9.1",
    "gulp-cache": "^0.4.6",
    "gulp-csso": "^3.0.0",
    "gulp-exit": "0.0.2",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.1.1",
    "gulp-load-plugins": "^1.5.0",
    "gulp-plumber": "^1.1.0",
    "gulp-rev": "^7.1.2",
    "gulp-rev-replace": "^0.4.3",
    "gulp-sass": "^3.1.0",
    "gulp-size": "^2.1.0",
    "gulp-sourcemaps": "^2.4.1",
    "gulp-uglify": "^2.1.2",
    "gulp-useref": "^3.1.2",
    "gulp-util": "^3.0.8",
    "jeet": "^7.2.0",
    "node-bourbon": "^4.2.8",
    "node-notifier": "^5.1.2",
    "node-sass": "^4.5.1",
    "require-globify": "^1.4.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0",
    "watchify": "^3.9.0"
  },
  "dependencies": {
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "mapbox-gl": "^0.39.1",
    "mapbox-gl-compare": "^0.1.0",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "next",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0"
  }
}
